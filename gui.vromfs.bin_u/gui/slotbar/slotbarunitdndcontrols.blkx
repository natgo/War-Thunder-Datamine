{
  "root": {
    "behaviour": [
      "button",
      "accesskey"
    ],
    "accessKey": "Esc",
    "on_click": "onTakeCancel",
    "blur": {},
    "blur_foreground": {
      "isRoot": "yes"
    },
    "div": {
      "id": "slotbar_with_controls",
      "width": "1@slotbarWidthFull",
      "position": "absolute",
      "left": "50%pw-50%w",
      "top": "1@unitTrashBinPosY - h/2 - 12@sf/@pf",
      "flow": "vertical",
      "tdiv": [
        {
          "margin-bottom": "20@sf/@pf",
          "halign": "center",
          "selectCrewLegend": {
            "id": "qualification_legend"
          }
        },
        {
          "position": "relative",
          "pos": "50%pw-50%w, @itemsIntervalBig",
          "padding-left": "0.5*@scrn_tgt/100.0",
          "tdiv": {
            "id": "set_air_dnd_title",
            "position": "relative",
            "left": "50%pw-50%w",
            "height": "40@sf/@pf",
            "textAreaCentered": {
              "id": "set_air_dnd_title_text",
              "class": "active",
              "text": ""
            }
          }
        }
      ],
      "slotbarNest": {
        "id": "take-aircraft-slotbar",
        "class": "selectCrew",
        "pos": "50%pw-50%w, 0",
        "position": "relative"
      }
    },
    "unitTrashBin": {
      "id": "unitTrashBin",
      "css-hier-invalidate": "yes",
      "img": {
        "background-image": "#ui/gameuiskin#icon_trash_bin.svg",
        "size": "@trashBinIconSize, @trashBinIconSize",
        "background-svg-size": "@trashBinIconSize, @trashBinIconSize",
        "halign": "center"
      },
      "textareaNoTab": {
        "id": "trashBinText",
        "text": "#slotbar/removeVehicle",
        "width": "0.9pw",
        "margin-top": "1@blockInterval",
        "halign": "center",
        "text-align": "center",
        "smallFont": "yes"
      }
    }
  }
}